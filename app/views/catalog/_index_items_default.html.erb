<!-- TODO: Use a different partial for serials? -->
<!--       Skip display if holdings are only ONLINE; this is an NCSU-specific hack atm; we might also skip showing print summaries for journals -->

<% if document.findingaid_urls.any? || document.fulltext_urls.any? || document.holdings.keys.any? %>
  <% unless ['ONLINE'] == document.holdings.keys %>
    <%= render partial: "items_section", locals: { document: document, context: 'index' } %>
  <% end %>
<% end %>
